{
  "extends": ["airbnb"],

  "env": {
    "browser": true,
    "node": true
  },

  "globals": {
    "Lang": true,
  },

  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },

  "rules": {
    "semi": [ "error", "never" ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-prototype-builtins": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "sx-a11y/anchor-has-content": 0,
    "react/prop-types": 0,
    "consistent-return": 0,
    "react/no-array-index-key": 0,
    "max-len": 0,
    "prefer-default-export": 0,
  }
}
