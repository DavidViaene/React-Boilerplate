{
  "extends": ["airbnb"],

  "env": {
    "browser": true,
    "node": true
  },

  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },

  "rules": {
    "semi": [ "error", "never" ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-prototype-builtins": 0,
    "react/forbid-prop-types": 0,
    "react/prop-types": 0,
    "no-trailing-spaces": 0
  }
}
